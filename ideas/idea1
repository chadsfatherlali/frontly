`<!DOCTYPE html>
    <html>
    <head>
        <title>Document</title>
    </head>
    <body>
      <p>Hola {{name}}</p>
      {{> button}}
    </body>
    </html>`;

    Handlebars.registerPartial(
      'button',
      `<button id="alertMessage">Click me!</button>
      <script>
        document.querySelector('#alertMessage').onclick = () => {
          alert('Hola {{name}}')
        }
      </script>`,
    );